DOMAINS
город = symbol
страна = symbol

PREDICATES
nondeterm расположение(город, страна)

CLAUSES
расположение("Москва", "Россия").
расположение("Санкт-Петербург", "Россия").
расположение("Париж", "Франция").
расположение("Берлин", "Германия").
расположение("Пхеньян", "Корея").
расположение("Нью-Йорк", "США").
расположение("Рио-де-Жанейро", "Бразилия").

расположение(X, "Европа") :- расположение(X, "Россия").
расположение(X, "Европа") :- расположение(X, "Франция").
расположение(X, "Европа") :- расположение(X, "Германия").
расположение(X, "Азия") :- расположение(X, "Корея").
расположение(X, "Северная Америка") :- расположение(X, "США").
расположение(X, "Южная Америка") :- расположение(X, "Бразилия").

GOAL
расположение(, "Европа").